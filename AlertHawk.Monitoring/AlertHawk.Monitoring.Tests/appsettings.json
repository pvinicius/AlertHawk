{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RabbitMq": {
    "Host": "localhost",
    "User": "guest",
    "Pass": "guest"
  },
  "ConnectionStrings": {
    "SqlConnectionString": ""
  },
  "CacheSettings": {
    "CacheProvider": "Redis",
    "IsDistributed": true,
    "RedisConnectionString": "localhost:6379"
  },
  "SwaggerUICredentials": {
    "username": "",
    "password": ""
  },
  "Sentry": {
    "Dsn": "",
    "MaxRequestBodySize": "Always",
    "Environment": "Development",
    "SendDefaultPii": true,
    "MinimumBreadcrumbLevel": "Error",
    "MinimumEventLevel": "Error",
    "AttachStackTrace": true,
    "Debug": false,
    "DiagnosticsLevel": "Error"
  },
  "CustomLogger": {
    "Host": "LoggerUrl",
    "Environment": "Production",
    "LogLevel": "Debug"
  }
}
